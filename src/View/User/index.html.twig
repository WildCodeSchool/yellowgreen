{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Users</h1>
    <ul>
        {% for user in users %}
            <li><a href="/users/show?id={{ user.id }}">{{ user.id }}. {{ user.nickName }}</a></li>
        {% else %}
            <li>Nothing to display</li>
        {% endfor %}
    </ul>
    <a href="/users/add">Ajouter</a>
{% endblock %}