{% block content %}
	<section class="formSection">
		<form class="formSection login" method="post" enctype="multipart/form-data" action="/login">

			<label class="email-login-label" for="email" display="hidden" required></label>
			<input type="text" value="" name="email" id="email" placeholder="Email">

			<label for="password" display="hidden" required></label>
			<input type="text" value="" name="password" id="password" placeholder="Password">

			<button class="button" type="submit">Je me connecte !</button>
		</form>
	</section>

{% endblock %}
